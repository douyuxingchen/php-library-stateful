{
    "name": "douyuxingchen/php-library-stateful",
    "description": "有状态的库旨在简化您的业务代码和优化您的业务架构，将底层基础设施沉淀到基础库，如 Redis 分布式锁定，数据查询缓存等等。",
    "type": "library",
    "license": "GPL-3.0-only",
    "autoload": {
        "psr-4": {
            "Douyuxingchen\\PhpLibraryStateful\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "libin",
            "email": "libin@douyuxingchen.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "ext-json": "*",
        "laravel/framework": "^6.0",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    }
}
